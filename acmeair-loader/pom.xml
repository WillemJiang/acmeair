		</dependency>
		<dependency>
			<groupId>au.com.dius</groupId>
			<artifactId>pact-jvm-consumer-junit_2.11</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>au.com.dius</groupId>
			<artifactId>pact-jvm-model_2.11</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.github.seanyinx</groupId>
			<artifactId>unit-scaffolding</artifactId>
			<scope>test</scope>
		</dependency>
